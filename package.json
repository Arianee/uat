{
  "name": "@arianee/uat",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "test": "npm run test:unit && node test.js",
    "server": "node_modules/lite-server/bin/lite-server -c lite-server.conf.json",
    "test:unit": "jest",
    "test:e2e": "./node_modules/.bin/cucumber-js features/**.feature --require-module ts-node/register --require 'src/steps/**/*.step.ts' --format node_modules/cucumber-pretty",
