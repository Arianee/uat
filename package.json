  "devDependencies": {
    "@types/node": "^13.1.8",
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@types/cucumber": "^6.0.0",
    "@types/jest": "^24.9.0",
    "babel-jest": "^25.1.0",
    "babel-watch": "^7.0.0",
    "jest": "^25.1.0",
