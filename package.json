{
  "name": "@arianee/uat",
  "version": "2.22.0",
  "description": "",
  "main": "dist",
  "scripts": {
    "preversion": "gitool -pao=changelog.md --tag=1",
    "prepublish": "npm run build && npm test",
    "postpublish": "npm version minor && git push",
    "build": "tsc",
    "build:w": "tsc --watch",
    "test": "npm run test:unit && npm run test:e2e",
    "server": "lite-server -c lite-server.config.js",
    "test:unit": "./node_modules/.bin/jest",
    "test:e2e": "node index.bin.js ./features/**.feature",
    "e2e:dev": "headless=false npm run test:e2e -- --tags @dev"
  },
  "bin": {
    "uat": "index.bin.js"
  },
  "files": [
    "common_steps",
    "features",
    "script.js"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "7.2.x",
    "@cucumber/pretty-formatter": "1.0.0-alpha.0",
    "@slack/web-api": "6.1.x",
    "faker": "5.2.x",
    "form-data": "3.0.x",
    "gently-copy": "3.2.x",
    "lite-server": "^2.6.1",
    "node-fetch": "2.6.x",
    "onecolor": "3.1.x",
    "playwright": "1.10.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-transform-async-to-generator": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "babel-jest": "^25.1.0",
    "babel-watch": "^7.0.0",
    "gitool": "^1.17.0",
    "jest": "^25.1.0",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
